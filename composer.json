{
  "name": "qase/qase-api-v2-client",
  "description": "Qase TestOps API V2 client for PHP",
  "homepage": "https://developers.qase.io",
  "license": "Apache-2.0",
  "type": "library",
  "keywords": [
    "qase",
    "tms",
    "api",
    "client",
    "php"
  ],
  "authors": [
    {
      "name": "Qase Team",
      "email": "support@qase.io"
    }
  ],
  "require": {
    "php": "^8.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "guzzlehttp/guzzle": "^7.3",
    "guzzlehttp/psr7": "^2.1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  },
  "autoload": {
    "psr-4": {
      "Qase\\APIClientV2\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "test/"
    }
  },
  "version": "1.1.0",
  "scripts": {
    "test": "phpunit"
  },
  "config": {
    "platform": {
      "php": "8.0"
    },
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    },
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  }
}
